{
  "name": "fronvo",
  "version": "0.0.1",
  "description": "The server of the open-source free-convo chat app.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fronvo/fronvo.git"
  },
  "author": "Shadofer#6681",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Fronvo/fronvo/issues"
  },
  "homepage": "https://github.com/Fronvo/fronvo#readme",
  "private": true,
  "scripts": {
    "start": "node output/main/server.js",
    "build": "tsc; copyfiles generated/*.json output/",
    "test:mocha": "mocha \"output/test/**/*.js\"",
    "test:server": "FRONVO_TEST_MODE=true FRONVO_LOCAL_SAVE=false FRONVO_BINARY_PARSER=false FRONVO_SILENT_LOGGING=true node output/main/server.js",
    "test": "npm run build; run-p test:server test:mocha"
  },
  "dependencies": {
    "@socket.io/admin-ui": "^0.2.0",
    "@socket.io/pm2": "^5.1.0",
    "dotenv": "^10.0.0",
    "eiows": "^3.6.1",
    "gradient-string": "^1.2.0",
    "joi": "^17.4.2",
    "mongodb": "^4.1.3",
    "ora": "5.4.1",
    "socket.io": "^4.2.0",
    "socket.io-msgpack-parser": "^3.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "copyfiles": "^2.4.1",
    "mocha": "^9.2.1",
    "npm-run-all": "^4.1.5",
    "socket.io-client": "^4.2.0",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  }
}
