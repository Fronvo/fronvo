{
  "name": "fronvo",
  "version": "0.2.0",
  "description": "The server of the open-source free-convo chat app.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fronvo/fronvo.git"
  },
  "author": "Shadofer#6681",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Fronvo/fronvo/issues"
  },
  "homepage": "https://github.com/Fronvo/fronvo#readme",
  "private": true,
  "scripts": {
    "build": "swc src -d output && copyfiles -u 1 src/generated/** src/generated/**/* output",
    "start": "run-s build start:cached",
    "start:cached": "node -r @swc-node/register -r tsconfig-paths/register output/main/server.js",
    "start:dev": "tsnd --transpile-only --respawn --exit-child --clear -r tsconfig-paths/register src/main/server.ts",
    "test": "run-p test:server test:mocha",
    "test:mocha": "mocha -r @swc-node/register -r tsconfig-paths/register src/test/main.ts",
    "test:server": "cross-env FRONVO_TEST_MODE=true FRONVO_LOCAL_SAVE=false FRONVO_BINARY_PARSER=false FRONVO_SILENT_LOGGING=true node -r @swc-node/register -r tsconfig-paths/register src/main/server.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "@socket.io/admin-ui": "^0.2.0",
    "@socket.io/pm2": "^5.1.0",
    "@swc-node/register": "^1.4.2",
    "@swc/cli": "^0.1.57",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "ez-ratelimiter": "^0.3.0",
    "gradient-string": "^1.2.0",
    "joi": "^17.6.0",
    "npm-run-all": "^4.1.5",
    "ora": "^5.4.1",
    "socket.io": "^4.4.1",
    "socket.io-msgpack-parser": "^3.0.1",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/gradient-string": "^1.1.2",
    "@types/mocha": "^9.1.0",
    "@types/uuid": "^8.3.4",
    "mocha": "^9.2.2",
    "prisma": "^3.12.0",
    "socket.io-client": "^4.4.1",
    "ts-node-dev": "^1.1.8"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  }
}
